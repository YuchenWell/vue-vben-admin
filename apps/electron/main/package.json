{
  "name": "@yyb-electron/main",
  "version": "1.0.0",
  "description": "运营宝MVP",
  "private": true,
  "author": "海创星智",
  "type": "commonjs",
  "scripts": {
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "pack:app": "npx tsx scripts/build.ts",
    "pack:dev": "pnpm build:dev && dotenvx run -f .env.development -- pnpm pack:app",
    "pack:prod": "pnpm build:prod && dotenvx run -f .env.production -- pnpm pack:app",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "postinstall": "cross-env ELECTRON_RUN_AS_NODE=1 electron ./scripts/update-electron-vendors.mjs"
  },
  "main": "dist/index.cjs",
  "dependencies": {
    "chalk": "catalog:",
    "electron-log": "catalog:"
  },
  "devDependencies": {
    "@electron/notarize": "catalog:",
    "@types/node": "catalog:",
    "@vben/eslint-config": "workspace:",
    "@vben/node-utils": "workspace:*",
    "@yyb-electron/preload": "workspace:*",
    "electron": "catalog:",
    "electron-builder": "catalog:",
    "electron-updater": "catalog:",
    "typescript": "catalog:",
    "vite": "catalog:"
  }
}
